APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_HOST=localhost
APP_URL=http://${APP_HOST}
APP_HTTPS=false

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=
# DB_PREFIX=laravel_
# DB_PREFIX_ADMIN=admin_
# DB_PREFIX_API=api_
# DB_PREFIX_WEB=web_

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

OCTANE_SERVER=swoole

# sql log
LOG_SQL=false

# web route config
WEB_ROUTE_DOMAIN=
WEB_ROUTE_PREFIX=

# api route config
# access api.localhost when domain is set
API_ROUTE_DOMAIN=api.${APP_HOST}
# access localhost/api when prefix is set
API_ROUTE_PREFIX=api

# admin route config
# access admin.localhost when domain is set
ADMIN_ROUTE_DOMAIN=admin.${APP_HOST}
# access localhost/admin when prefix is set
ADMIN_ROUTE_PREFIX=admin

# admin front-end vendor config
ADMIN_VENDOR_PATH=build
ADMIN_VENDOR_FRAMEWORK=build/Pear-Admin-Layui
ADMIN_VENDOR_CONFIG_FILE=config/pear.config.json
ADMIN_VENDOR_CONFIG_URL=/config
ADMIN_LOGO=/build/pear-admin-site/assets/images/logo3.png
